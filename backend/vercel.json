{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/index.js"
    }
  ],
  "env": {
    "PG_HOST": "@pg_host",
    "PG_PORT": "@pg_port",
    "PG_DATABASE": "@pg_database",
    "PG_USER": "@pg_user",
    "PG_PASSWORD": "@pg_password",
    "PG_SSL": "@pg_ssl"
  }
}
